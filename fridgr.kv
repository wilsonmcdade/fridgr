BoxLayout:
    orientation: 'horizontal'
    FloatLayout:
        size_hint: .1,1
        Label:
            text: "Left"
        Button:
            anchor_y: "top"
            text: "Top"
            pos_hint: {'top':1}
            size_hint: 1,.2
            multiselect: True 
            touch_multiselect: True 
   
        Button:
            anchor_y: "center"
            text: "Center"
            pos_hint: {'top':.8}
            size_hint: 1,.2
            multiselect: True 
            touch_multiselect: True 
 
    AnchorLayout:
        size_hint: 1,1
        anchor_x: "center"
        Label:
            text: "Fridgr"


